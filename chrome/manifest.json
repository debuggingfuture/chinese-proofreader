{
  "manifest_version": 2,

  "name": "Chinese-proofreader",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "content_scripts": [
    {
    "matches": ["*://*/*"],
    "css": ["style.css"],
    "js": ["content.js"]
    }
  ],

  "permissions": [
    "activeTab",
    "http://ajax.googleapis.com/"
  ]
}

{
  "manifest_version": 2,

  "name": "Chinese-proofreader",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  
  "content_scripts": [
    {
    "matches": ["http://*/*"],
    "css": ["style.css"],
    "js": ["jquery-3.0.0.min.js", "content.js"]
    }
  ],

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}